.donate-form-input.donate-form-input-select
    svg(width='35', height='35', viewbox='0 0 35 35', fill='none', xmlns='http://www.w3.org/2000/svg', xmlns:xlink='http://www.w3.org/1999/xlink')
        mask#mask-donate-input-server-svg(mask-type='alpha', maskunits='userSpaceOnUse', x='0', y='0', width='35', height='35')
            rect(width='35', height='35', fill='url(#parrent-donate-input-server-svg)')
        g(mask='url(#mask-donate-input-server-svg)')
            rect(width='35', height='35', fill='url(#gradient-donate-input-server-svg)')
        defs
            pattern#parrent-donate-input-server-svg(patterncontentunits='objectBoundingBox', width='1', height='1')
                use(xlink:href='#image-donate-input-server-svg', transform='scale(0.00195312)')
            lineargradient#gradient-donate-input-server-svg(x1='28.2828', y1='5.54332e-06', x2='2.37705', y2='1.2858', gradientunits='userSpaceOnUse')
                stop(stop-color='#F8CE41')
                stop(offset='1', stop-color='#FFA010')
            image#image-donate-input-server-svg(width='512', height='512', xlink:href='data:image/png;base64,')

    .donate-select#donate-input-server Выберите сервер
    .donate-select-list
        .donate-select-item(data-id="0") Enki Online Plus Test
        .donate-select-item(data-id="1") Enki Online Plus #1
        

